{
  "name": "independer-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "build:prod": "npm run clean && webpack --env.PROD --env.AOT",
    "build:analyze": "npm run build -- --env.ANALYZE",
    "build:prod:analyze": "npm run build:prod -- --env.ANALYZE",
    "build:server": "webpack --env.SERVER",
    "build:server:prod": "webpack --env.PROD --env.AOT --env.SERVER",
    "clean:aot": "npm run rimraf -- aot_temp",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "clean:serverdist": "npm run rimraf -- wwwroot/serverdist",
    "clean:dll": "rimraf -- wwwroot/dll_dev",
    "clean:npm": "npm cache clean && npm uninstall rimraf && rimraf -- node_modules",
    "clean:coverage": "rimraf -- wwwroot/coverage",
    "clean": "rimraf -- wwwroot/dist wwwroot/serverdist aot_temp wwwroot/coverage",
    "re-install": "npm run clean:npm && npm install",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "start": "dotnet run",
    "start:dev": "npm run env:dev dotnet watch run",
    "start:prod": "npm run env:prod dotnet run",
    "env:dev": "cross-env ASPNETCORE_ENVIRONMENT=Development",
    "env:prod": "cross-env ASPNETCORE_ENVIRONMENT=Production",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "postinstall": "npm run webdriver:update",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "e2e:dev": "npm run clean && npm-run-all -p -r start:dev protractor:delay",
    "e2e:prod": "npm run clean && npm run build:prod && npm-run-all -p -r start:prod protractor",
    "ci": "npm run clean && npm run lint && npm run build && npm run test && npm run build:server && npm run build:prod && npm run build:server:prod"
  },
  "dependencies": {
    "@angular/animations": "4.4.4",
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/platform-server": "4.4.4",
    "@angular/router": "4.4.4",
    "@ngx-meta/core": "4.0.1",
    "aspnet-prerendering": "3.0.1",
    "core-js": "2.5.1",
    "ie-shim": "0.1.0",
    "jasmine-core": "2.8.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.4.4",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "2.5.43",
    "@types/node": "8.0.32",
    "@types/selenium-webdriver": "3.0.7",
    "@types/source-map": "0.5.1",
    "@types/uglify-js": "2.6.29",
    "@types/webpack": "3.0.13",
    "angular2-template-loader": "0.6.2",
    "aspnet-webpack": "2.0.1",
    "circular-dependency-plugin": "4.2.1",
    "clean-webpack-plugin": "0.1.17",
    "codelyzer": "3.2.0",
    "compression-webpack-plugin": "1.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "0.2.8",
    "happypack": "4.0.0",
    "istanbul-instrumenter-loader": "3.0.0",
    "json-loader": "0.5.7",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.4",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "ng-router-loader": "2.1.0",
    "ngc-webpack": "3.0.0",
    "node-sass": "4.5.3",
    "npm-run-all": "4.1.1",
    "optimize-js-plugin": "0.0.4",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.2",
    "style-loader": "0.19.0",
    "to-string-loader": "1.1.5",
    "ts-loader": "2.3.7",
    "ts-node": "3.3.0",
    "tslib": "1.7.1",
    "tslint": "5.7.0",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.5.3",
    "url-loader": "0.6.2",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-hot-middleware": "2.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/independer/ind-angular-starter.git"
  },
  "engines": {
    "node": ">= 6.9.4",
    "npm": ">= 3"
  }
}
