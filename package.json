{
  "name": "independer-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "build:prod": "npm run build:aot:prod && npm run build:server:prod",
    "build:aot": "webpack && npm run clean:aot",
    "build:aot:prod": "webpack && npm run clean:aot",
    "build:server": "webpack",
    "build:server:prod": "webpack",
    "clean:aot": "npm run rimraf -- aot_temp",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "clean:serverdist": "npm run rimraf -- wwwroot/serverdist",
    "clean:dll": "rimraf -- wwwroot/dll_dev",
    "clean:install": "npm set progress=false && npm install",
    "clean:npm": "npm cache clean && npm uninstall rimraf && rimraf -- node_modules",
    "clean:coverage": "rimraf -- wwwroot/coverage",
    "clean": "rimraf -- wwwroot/dist wwwroot/serverdist aot_temp wwwroot/coverage",
    "re-install": "npm run clean:npm && npm install",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "webpack": "webpack",
    "start": "dotnet run",
    "start:dev": "npm run env:dev dotnet watch run",
    "start:prod": "npm run env:prod dotnet run",
    "env:dev": "cross-env ASPNETCORE_ENVIRONMENT=Development",
    "env:prod": "cross-env ASPNETCORE_ENVIRONMENT=Production",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "postinstall": "npm run webdriver:update",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "e2e:dev": "npm run clean && npm-run-all -p -r start:dev protractor:delay",
    "e2e:prod": "npm run clean && npm run build:prod && npm-run-all -p -r start:prod protractor",
    "ci": "npm run clean && npm run build && npm run test && npm run e2e:dev && npm run e2e:prod && npm run clean"
  },
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/platform-server": "2.4.9",
    "@angular/router": "3.4.9",
    "angular2-universal": "2.1.0-rc.1",
    "angular2-universal-polyfills": "2.1.0-rc.1",
    "aspnet-prerendering": "2.0.3",
    "core-js": "2.4.1",
    "ie-shim": "0.1.0",
    "jasmine-core": "2.5.2",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.9",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "@types/selenium-webdriver": "2.53.40",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.2.7",
    "angular2-template-loader": "0.6.2",
    "aspnet-webpack": "1.0.28",
    "awesome-typescript-loader": "3.0.8",
    "clean-webpack-plugin": "0.1.15",
    "codelyzer": "2.0.1",
    "compression-webpack-plugin": "0.3.2",
    "cross-env": "3.1.4",
    "css-loader": "0.26.2",    
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "ng-router-loader": "2.1.0",
    "ngc-webpack": "1.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "optimize-js-plugin": "0.0.4",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "4.1.1",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "to-string-loader": "1.1.5",
    "ts-node": "2.1.0",
    "tslib": "1.6.0",
    "tslint": "4.4.2",
    "tslint-eslint-rules": "3.4.0",
    "tslint-loader": "3.4.3",
    "typescript": "2.2.1",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "2.3.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-dev-server": "2.4.1",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-hot-middleware": "2.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/independer/ind-angular-starter.git"
  },
