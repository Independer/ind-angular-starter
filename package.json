{
  "name": "independer-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run webpack",
    "build:analyze": "npm run webpack -- --env.ANALYZE",
    "build:no-dev-dll": "npm run webpack -- --env.NO_DEV_DLL",
    "build:prod": "npm run webpack -- --env.PROD --env.AOT && npm run clean:aot",
    "build:prod:analyze": "npm run webpack -- --env.PROD --env.AOT --env.ANALYZE && npm run clean:aot",
    "build:server": "npm run webpack -- --env.SERVER",
    "build:server:prod": "npm run webpack -- --env.PROD --env.AOT --env.SERVER && npm run clean:aot",
    "clean:aot": "rimraf -- aot_temp",
    "clean:dist": "rimraf -- wwwroot/dist",
    "clean:dll": "rimraf -- wwwroot/dll_dev",
    "clean:install": "npm set progress=false && npm install",
    "clean:npm": "npm cache clean && npm uninstall rimraf && rimraf -- node_modules",
    "clean:coverage": "rimraf -- wwwroot/coverage",
    "clean": "rimraf -- wwwroot/dist wwwroot/serverdist aot_temp wwwroot/coverage",
    "gulp": "gulp",
    "webpack": "node --max_old_space_size=8192 node_modules/webpack/bin/webpack.js",
    "generate-iconfont": "npm run gulp generate-iconfont",
    "re-install": "npm run clean:npm && npm install",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "start": "dotnet run",
    "start:dev": "npm run env:dev dotnet run",
    "start:prod": "npm run env:prod dotnet run",
    "env:dev": "cross-env ASPNETCORE_ENVIRONMENT=Development",
    "env:prod": "cross-env ASPNETCORE_ENVIRONMENT=Production",
    "start:dev:node": "webpack-dev-server --config webpack.config.js --watch --content-base wwwroot",
    "rimraf": "rimraf",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "ci": "npm run clean && npm run lint && npm run build && npm run test && npm run build:prod && npm run build:server:prod"
  },
  "dependencies": {
    "@angular/animations": "4.4.3",
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@angular/platform-server": "4.4.3",
    "@angular/router": "4.4.3",
    "@ngx-meta/core": "4.0.1",
    "aspnet-prerendering": "3.0.1",
    "core-js": "2.5.1",
    "jasmine-core": "2.8.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@types/jasmine": "2.6.0",
    "@types/node": "8.0.28",
    "add-asset-html-webpack-plugin": "2.1.2",
    "angular-named-lazy-chunks-webpack-plugin": "^1.0.0",
    "angular2-template-loader": "0.6.2",
    "aspnet-webpack": "2.0.1",
    "circular-dependency-plugin": "4.2.1",
    "clean-webpack-plugin": "0.1.16",
    "codelyzer": "3.2.0",
    "compression-webpack-plugin": "1.0.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "0.2.8",
    "gulp": "^3.9.1",
    "gulp-iconfont": "9.0.1",
    "gulp-iconfont-css": "2.1.0",
    "gulp-replace": "0.6.1",
    "happypack": "4.0.0",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "json-loader": "0.5.7",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.4",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "ng-router-loader": "2.1.0",
    "ngc-webpack": "3.0.0",
    "node-sass": "4.5.3",
    "optimize-js-plugin": "0.0.4",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.1",
    "sass-lint": "1.11.1",
    "sass-loader": "6.0.6",
    "sass-resources-loader": "1.3.0",
    "shortid": "2.2.8",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "to-string-loader": "1.1.5",
    "ts-loader": "2.3.7",
    "tslib": "1.7.1",
    "tslint": "5.7.0",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.5.3",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.8.2",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-livereload-plugin": "0.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/independer/ind-angular-starter.git"
  },
  "engines": {
    "node": ">= 6.9.4",
    "npm": ">= 3"
  }
}
