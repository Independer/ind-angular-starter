{
  "name": "independer-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "build:prod": "npm run clean && npm run build:aot:prod",
    "build:aot": "webpack && npm run clean:aot",
    "build:aot:prod": "webpack && npm run clean:aot",
    "clean:aot": "npm run rimraf -- aot_temp",
    "clean:dist": "npm run rimraf -- wwwroot/dist",
    "clean:dll": "rimraf -- wwwroot/dll_dev",
    "clean:install": "npm set progress=false && npm install",
    "clean:npm": "npm cache clean && npm uninstall rimraf && rimraf -- node_modules",
    "clean:coverage": "rimraf -- wwwroot/coverage",
    "clean": "rimraf -- wwwroot/dist aot_temp wwwroot/coverage",
    "re-install": "npm run clean:npm && npm install",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "webpack": "webpack",
    "start": "dotnet run",
    "start:dev": "npm run env:dev dotnet watch run",
    "start:prod": "npm run env:prod dotnet run",
    "env:dev": "cross-env ASPNETCORE_ENVIRONMENT=Development",
    "env:prod": "cross-env ASPNETCORE_ENVIRONMENT=Production",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "postinstall": "npm run webdriver:update",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "e2e:dev": "npm run clean && npm-run-all -p -r start:dev protractor:delay",
    "e2e:prod": "npm run clean && npm run build:prod && npm-run-all -p -r start:prod protractor",
    "ci": "npm run clean && npm run build && npm run test && npm run e2e:dev && npm run e2e:prod"
  },
  "dependencies": {
    "@angular/animations": "4.0.1",
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",    
    "@angular/core": "4.0.1",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",    
    "@angular/router": "4.0.1",
    "core-js": "2.4.1",
    "ie-shim": "0.1.0",
    "jasmine-core": "2.5.2",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.3.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {    
    "@angular/compiler-cli": "4.0.1",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.12",
    "@types/selenium-webdriver": "3.0.1",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.2.14",
    "angular2-template-loader": "0.6.2",
    "aspnet-webpack": "1.0.28",
    "awesome-typescript-loader": "3.1.2",
    "clean-webpack-plugin": "0.1.16",
    "codelyzer": "2.1.1",
    "compression-webpack-plugin": "0.4.0",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",    
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.3",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "ng-router-loader": "2.1.0",
    "ngc-webpack": "1.2.0",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "optimize-js-plugin": "0.0.4",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "source-map-loader": "0.2.1",
    "style-loader": "0.16.1",
    "to-string-loader": "1.1.5",
    "ts-node": "3.0.2",
    "tslib": "1.6.0",
    "tslint": "4.5.1",
    "tslint-eslint-rules": "3.5.1",
    "tslint-loader": "3.5.2",
    "typescript": "2.2.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-bundle-analyzer": "2.3.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-hot-middleware": "2.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/independer/ind-angular-starter.git"
  },
  "engines": {
    "node": ">= 6.9.4",
    "npm": ">= 3"
  }
}
